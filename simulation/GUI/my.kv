<MainWindow>:
    BoxLayout:
        cols: 2
        TabbedPanel:
            size_hint: (.7, 1)
            do_default_tab: False
            tab_width: self.width / 2 #tabs fill entire width of TabbedPanel
            TabbedPanelItem:
                text: 'Map'
                WelcomePanel:
            TabbedPanelItem:
                text: 'tab2'
                Label:
                    text: '??? place for house ???'
        ConfigPanel:


<WelcomePanel@RelativeLayout>:
    Label:
        pos_hint: {'center_x': 0.5, 'center_y': 0.55}
        text: 'Welcome to application! \n To start simulation upload map'
        halign: 'center'
        font_size: 18
    Button:
        size_hint: .3, .1
        pos_hint: {'center_x': 0.5, 'center_y': 0.4}
        text: 'upload'
        font_size: 18
        on_release: app.open_chooser()

<ConfigPanel@RelativeLayout>:
    size_hint: .3, 1
    Label:
        text: 'config'
